# BlockLucky Environment Variables
# Copy this file to .env.local and fill in your values

# Contract address (deployed smart contract)
NEXT_PUBLIC_CONTRACT_ADDRESS=

# Chain ID (1 for Mainnet, 11155111 for Sepolia, 31337 for Hardhat Local)
NEXT_PUBLIC_CHAIN_ID=31337

# Etherscan URL (optional, defaults to etherscan.io)
NEXT_PUBLIC_ETHERSCAN_URL=
